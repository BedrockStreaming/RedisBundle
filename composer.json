{
    "name": "m6web/redis-bundle",
    "type": "symfony-bundle",
    "description" : "bundle sf on top of predis",
    "authors": [
        {
            "name": "Olivier Mansour",
            "email": "olivier.mansour@m6.fr"
        }
    ],
    "config": {
        "vendor-dir": "vendor",
        "bin-dir": "bin"
    },
    "require": {
        "php": "^7.4",
        "predis/predis": "^1.1.1",
        "doctrine/cache": "~1.3",
        "b1rdex/predis-compressible": "^0.3.0"
    },
    "require-dev": {
        "m6web/php-cs-fixer-config": "^1.0",
        "atoum/atoum": "^3.3.0",
        "atoum/atoum-bundle": "^2.0",
        "atoum/stubs": "~2.5",

        "m6web/redis-mock": "@stable",
        "symfony/symfony": "~5.0",
        "psr/cache": "~1.0"
    },
    "autoload": {
        "psr-4": { "M6Web\\Bundle\\RedisBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "M6Web\\Bundle\\RedisBundle\\Tests\\": "src/Tests/" }
    }
}
