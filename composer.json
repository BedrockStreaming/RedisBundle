{
    "name": "m6web/redis-bundle",
    "type": "symfony-bundle",
    "description" : "bundle sf2 on top of m6web/redis-component",
    "authors": [
        {
            "name": "Olivier Mansour",
            "email": "olivier.mansour@m6.fr"
        }
    ],
    "require": {
        "php": ">=5.5",
        "m6web/redis-component": "~3.0",
        "doctrine/cache":        "~1.3"
    },
    "require-dev": {
        "atoum/atoum":      "@stable",
        "m6web/redis-mock": "@stable",
        "symfony/symfony": "~2.3",
        "m6web/coke" : "~1.2",
        "m6web/symfony2-coding-standard" : "~1.1"
    },
    "suggest": {
      "m6web/wsclient-bundle": "@stable",
      "m6web/aws-bundle":      "@stable"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    }
}
